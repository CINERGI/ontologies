        <owl:disjointWith rdf:resource="&CGI_Lith;F_GT10_LT60"/>
        <owl:disjointWith rdf:resource="&CGI_Lith;F_GT60"/>
        <dc:description>Feldspathoid minerals form between 0 and 10 percent of QAPF fraction</dc:description>
