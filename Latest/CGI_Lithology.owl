        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label>grain size category</rdfs:label>
        <rdfs:range rdf:resource="&CGI_Lith;AnyGrainSize"/>
        <rdfs:domain rdf:resource="&CGI_Lith;rock_material"/>
    </owl:ObjectProperty>
    


    <!-- http://CGI:LithologyOWL:201001#MetamorphicFacies -->

    <owl:ObjectProperty rdf:about="&CGI_Lith;MetamorphicFacies"/>
    


