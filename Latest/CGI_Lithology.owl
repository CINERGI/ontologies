        <owl:disjointWith rdf:resource="&CGI_Lith;ultramafic"/>
        <dc:description>greater than 35 percent mafic minerals by volume or greater than 40 percent mafic minerals by weight, and less than 90 percent by weight or volume.</dc:description>
    </owl:Class>
    


