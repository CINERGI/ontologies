        <skos:definition rdf:datatype="&xsd;string">the association Metadata shall link the OM_Observation to descriptive metadata.</skos:definition>
        <rdfs:comment>generic metadata about an observation</rdfs:comment>
        <rdfs:domain rdf:resource="&gf;AnyFeature"/>
        <rdfs:isDefinedBy rdf:resource="http://def.seegrid.csiro.au/isotc211/iso19156/2011/observation"/>
